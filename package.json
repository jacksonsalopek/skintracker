{
  "name": "skintracker",
  "version": "0.0.2",
  "description": "",
  "author": "jacksonsalopek",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "identify-circ-deps": "skott --fileExtensions=.ts,.tsx --displayMode=file-tree --showCircularDependencies",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/skintracker/main",
    "lint": "rome check src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/skintracker/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@nestjs/platform-fastify": "^10.1.3",
    "@nestjs/schedule": "^3.0.2",
    "@ogma/nestjs-module": "^5.1.1",
    "axios": "^1.4.0",
    "nest-commander": "^3.11.0",
    "notp": "^2.0.3",
    "puppeteer": "^21.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "terminal-kit": "^3.0.0",
    "thirty-two": "^1.0.2",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.3",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.28",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.3.1",
    "@types/notp": "^2.0.2",
    "@types/supertest": "^2.0.12",
    "@types/terminal-kit": "^2.5.1",
    "jest": "^29.6.2",
    "nest-commander-schematics": "^3.0.0",
    "nest-commander-testing": "^3.3.0",
    "rome": "12.1.3",
    "skott": "^0.28.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s?$": ["@swc/jest"]
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": ["<rootDir>/libs/", "<rootDir>/apps/"],
    "moduleNameMapper": {
      "^@skintracker/sdk(|/.*)$": "<rootDir>/libs/sdk/src/$1"
    }
  }
}
